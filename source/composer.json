{
    "name": "joellarson/phpstorm-phpunit-issue",
    "type": "project",
    "require": {
        "phpunit/phpunit": "^9.5",
        "phpunit/phpcov": "^8.2"
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
      },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    }
}
